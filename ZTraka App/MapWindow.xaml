                <SplineColorKeyFrame KeyTime="0:0:0.3" Value="#FF82D45E"/>
            </ColorAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="AssetLoaded" RepeatBehavior="Forever">
